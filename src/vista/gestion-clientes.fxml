<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="controlador.Controlador">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Agregar cliente" onAction="#onAgregarClienteClick"/>
    <Button text="Mostrar clientes" onAction="#onMostrarClientesClick"/>
    <Button text="Mostrar clientes Standard" onAction="#onStandardClick"/>
    <Button text="Mostrar clientes Premium" onAction="#onPremiumClick"/>
</VBox>
